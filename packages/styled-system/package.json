{
  "name": "@wrisk/styled-system",
  "version": "5.1.5",
  "description": "Responsive, theme-based style props for building design systems with React",
  "author": "Brent Jackson",
  "license": "MIT",
  "main": "dist/wrisk-styled-system.cjs.js",
  "module": "dist/wrisk-styled-system.esm.js",
  "types": "dist/wrisk-styled-system.cjs.d.ts",
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "files": [
    "src",
    "dist",
    "module",
    "LICENSE.md",
    "README.md"
  ],
  "keywords": [
    "typescript",
    "react",
    "css-in-js",
    "styled-components",
    "design-system"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/*.test.ts"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.20.7",
    "@emotion/core": "^10.0.10",
    "@emotion/is-prop-valid": "^0.8.1",
    "@emotion/memoize": "^0.7.1",
    "color": "^3.1.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@emotion/styled": "^10.0.14",
    "csstype": "^3.0.2",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
